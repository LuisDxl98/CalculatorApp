<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="#607D8B"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="right|center_vertical"
			android:orientation="horizontal">
			<TextView
				android:id="@+id/salida"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:gravity="right|bottom"
				android:textSize="30sp"
				android:textColor="#000000"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal"/>
		<LinearLayout
			android:id="@+id/linear4"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:background="#FFFFFF"
			android:gravity="right|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="80dp"
				android:layout_height="30dp"
				android:src="@drawable/imageoff_1"
				android:scaleType="fitCenter"/>
		</LinearLayout>

		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<TextView
				android:id="@+id/tce"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="CE"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/textview2"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:padding="8dp"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/tsqrt"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:padding="8dp"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/tdiv"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="รท"
				android:textColor="#000000"
				android:textSize="60sp" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/linear8"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<TextView
				android:id="@+id/t7"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="7"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t8"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="8"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t9"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="9"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/tmult"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="x"
				android:textColor="#000000"
				android:textSize="60sp" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/linear9"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<TextView
				android:id="@+id/t4"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="4"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t5"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="5"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t6"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="6"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/trest"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="-"
				android:textColor="#000000"
				android:textSize="60sp" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/linear10"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<TextView
				android:id="@+id/t1"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="1"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t2"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="2"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t3"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="3"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/tsum"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="+"
				android:textColor="#000000"
				android:textSize="60sp" />
		</LinearLayout>

		<LinearLayout
			android:id="@+id/linear11"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#FFFFFF"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<TextView
				android:id="@+id/textview21"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center|center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="."
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/t0"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="0"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/textview23"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:padding="8dp"
				android:textColor="#000000"
				android:textSize="60sp" />

			<TextView
				android:id="@+id/tigual"
				android:layout_width="80dp"
				android:layout_height="80dp"
				android:layout_weight="1"
				android:gravity="center_horizontal|center_vertical"
				android:onClick="onClickNumber"
				android:padding="8dp"
				android:text="="
				android:textColor="#000000"
				android:textSize="60sp" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
